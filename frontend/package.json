    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ctrl/ngx-emoji-mart": "^7.0.0",
    "angular2-query-builder": "^0.6.2",
    "aos": "^2.3.4",
    "chart.js": "^3.3.2",
    "emoji-mart": "^5.2.2",
    "firebase": "^9.9.2",
    "ngx-cookie-service": "^14.0.1",
