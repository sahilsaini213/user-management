{
  "name": "warehouse-management",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:proxy": "ng serve --host 0.0.0.0 --proxy-config src/proxy.conf.json",
    "build": "ng build --aot",
    "watch": "ng build --watch --configuration development",
